{
  "title": "iPlayer homepage",
  "services": {
    "filter": {
      "list": [],
      "time": {
        "from": "now-1h",
        "to": "now"
      }
    }
  },
  "rows": [
    {
      "title": "iPlayer Homepage",
      "height": "250px",
      "editable": true,
      "collapse": false,
      "collapsable": true,
      "panels": [
        {
          "span": 12,
          "editable": true,
          "type": "graphite",
          "x-axis": true,
          "y-axis": true,
          "scale": 1,
          "y_formats": ["none"],
          "grid": {
            "max": null,
            "min": 0
          },
          "resolution": 100,
          "lines": true,
          "fill": 1,
          "linewidth": 2,
          "points": false,
          "pointradius": 5,
          "bars": false,
          "stack": true,
          "spyable": true,
          "options": false,
          "legend": true,
          "interactive": true,
          "legend_counts": true,
          "timezone": "browser",
          "percentage": false,
          "zerofill": true,
          "nullPointMode": "null",
          "steppedLine": false,
          "tooltip": {
            "value_type": "cumulative",
            "query_as_alias": true
          },
          "targets": [
            {
              "target": "aliasSub(sortByMaxima(exclude(stats.timers.bbc.iplayer.live.*.*.build_time.count_ps,'json|error|ibl|programme_search|userActivity|tv|recommendations')), 'stats.timers.bbc.iplayer.live.(.+).build_time.count_ps', '\\1')"
            }
          ],
          "title": "Page views / second (stacked)"
      },{
        "span": 6,
        "editable": true,
        "type": "graphite",
        "x-axis": true,
        "y-axis": true,
        "scale": 1,
        "y_formats": ["none", "none"],
        "grid": {
          "max": null,
          "min": 0
        },
        "resolution": 100,
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "points": false,
        "pointradius": 5,
        "bars": false,
        "stack": false,
        "spyable": true,
        "options": false,
        "legend": true,
        "interactive": true,
        "legend_counts": true,
        "timezone": "browser",
        "percentage": false,
        "zerofill": true,
        "nullPointMode": "connected",
        "steppedLine": false,
        "tooltip": {
          "value_type": "cumulative",
          "query_as_alias": true
        },
        "targets": [
          {
            "target": "alias(sumSeries(stats.timers.bbc.iplayer.live.*.*.build_time.count_ps),'requests / s')"
          },
            {
              "target": "alias(scale(movingAverage(averageSeries(stats.timers.bbc.iplayer.live.*.*.build_time.median),10),0.1),'build time (s) moving average')"
            }
        ],
        "aliasColors": {
          "requests / s": "#6ED0E0",
          "build time (s) moving average": "#E24D42"
        },
        "title": "Requests and build time"
    },
        {
          "span": 6,
          "editable": true,
          "type": "graphite",
          "x-axis": true,
          "y-axis": true,
          "scale": 1,
          "y_formats": ["none", "none"],
          "grid": {
            "max": null,
            "min": 0
