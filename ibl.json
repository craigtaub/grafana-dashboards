{
    "title": "iBL",
    "services": {
        "filter": {
            "list": [],
            "time": {
                "from": "now-1h",
                "to": "now"
            }
        }
    },
    "rows": [
        {
            "title": "iBL",
            "height": "250px",
            "editable": true,
            "collapse": false,
            "collapsable": true,
            "panels": [
                {
                    "span": 12,
                    "editable": true,
                    "type": "graphite",
                    "x-axis": true,
                    "y-axis": true,
                    "scale": 1,
                    "y_formats": [
                        "none"
                    ],
                    "grid": {
                        "max": null,
                        "min": 0
                    },
                    "resolution": 100,
                    "lines": true,
                    "fill": 1,
                    "linewidth": 2,
                    "points": false,
                    "pointradius": 5,
                    "bars": false,
                    "stack": true,
                    "spyable": true,
                    "options": false,
                    "legend": true,
                    "interactive": true,
                    "legend_counts": true,
                    "timezone": "browser",
                    "percentage": false,
                    "zerofill": true,
                    "nullPointMode": "null",
                    "steppedLine": false,
                    "tooltip": {
                        "value_type": "cumulative",
                        "query_as_alias": true
                    },
                    "targets": [
                        {
                            "target": "aliasSub(sortByMaxima(exclude(stats.timers.bbc.ibl.live.responses.*.time.count_ps, 'status|api')), 'stats.timers.bbc.ibl.live.responses.(.+).time.count_ps', '\\1')"
                        },
                        {
                            "target": "aliasSub(sortByMaxima(stats.timers.bbc.ibl.live.responses.*.*.time.count_ps), 'stats.timers.bbc.ibl.live.responses.(.+).time.count_ps', '\\1')"
                        }
                    ],
                    "title": "Page views / second (stacked)"
                },
                {
                    "span": 6,
                    "editable": true,
                    "type": "graphite",
                    "x-axis": true,
                    "y-axis": true,
                    "scale": 1,
                    "y_formats": [
                        "none",
                        "none"
                    ],
                    "grid": {
                        "max": null,
                        "min": 0
                    },
                    "resolution": 100,
                    "lines": true,
                    "fill": 0,
                    "linewidth": 2,
                    "points": false,
                    "pointradius": 5,
                    "bars": false,
                    "stack": false,
                    "spyable": true,
                    "options": false,
                    "legend": true,
                    "interactive": true,
                    "legend_counts": true,
                    "timezone": "browser",
                    "percentage": false,
                    "zerofill": true,
                    "nullPointMode": "connected",
                    "steppedLine": false,
                    "tooltip": {
                        "value_type": "cumulative",
                        "query_as_alias": true
                    },
                    "targets": [
                        {
                            "target": "alias(sumSeries(stats.timers.bbc.ibl.live.nitro.*.count_ps),'requests / s')"
                        },
                        {
                            "target": "alias(movingAverage(averageSeries(stats.timers.bbc.ibl.live.nitro.*.mean),100)),'build time (s) moving average')"
                        }
                    ],
                    "aliasColors": {
                        "requests / s": "#6ED0E0",
                        "build time (s) moving average": "#E24D42"
                    },
                    "aliasYAxis": {
                        "requests / s": 2
                      },
                    "title": "Nitro requests and build time"
                },
                {
                    "span": 6,
                    "editable": true,
                    "type": "graphite",
                    "x-axis": true,
                    "y-axis": true,
                    "scale": 1,
                    "y_formats": [
                        "none",
                        "none"
                    ],
                    "grid": {
                        "max": null,
                        "min": 0
                    },
                    "resolution": 100,
                    "lines": true,
                    "fill": 1,
                    "linewidth": 2,
                    "points": false,
                    "pointradius": 5,
                    "bars": false,
                    "stack": false,
                    "spyable": true,
                    "options": false,
                    "legend": true,
                    "interactive": true,
                    "legend_counts": true,
                    "timezone": "browser",
                    "percentage": false,
                    "zerofill": true,
                    "nullPointMode": "connected",
                    "steppedLine": false,
                    "tooltip": {
                        "value_type": "cumulative",
                        "query_as_alias": true
                    },
                    "targets": [
                        {
                            "target": "alias(stats.counters.bbc.iplayer.live.exceptions.bambooError500.count, 'Bamboo 500s / s')"
                        },
                        {
                            "target": "alias(stats.counters.bbc.ibl.live.exceptions.critical_request_failure.count, 'iBL critical failure / s')"
                        }
                    ],
                    "aliasColors": {
                        "Bamboo 500s / s": "rgb(234, 184, 57)",
                        "iBL critical failure / s": "rgb(226, 77, 66)"
                    },
                    "aliasYAxis": {
                        "iBL critical failure / s": 2
                    },
                    "title": "Bamboo & iBL failures"
                },
                {
                    "span": 6,
                    "editable": true,
                    "type": "graphite",
                    "x-axis": true,
                    "y-axis": false,
                    "scale": 1,
                    "grid": {
                        "max": 0.9,
                        "min": 0.1
                    },
                    "resolution": 100,
                    "lines": true,
                    "fill": 5,
                    "linewidth": 2,
                    "points": false,
                    "pointradius": 5,
                    "bars": false,
                    "stack": false,
                    "spyable": true,
                    "options": false,
                    "legend": true,
                    "interactive": true,
                    "legend_counts": true,
                    "timezone": "browser",
                    "percentage": false,
                    "zerofill": true,
                    "nullPointMode": "connected",
                    "steppedLine": true,
                    "tooltip": {
                        "value_type": "cumulative",
                        "query_as_alias": true
                    },
                    "targets": [
                        {
                            "target": "aliasByMetric(stats.gauges.bbc.iplayer.ci.build.*)"
                        },
                        {
                            "target": "alias(drawAsInfinite(stats.counters.bbc.iplayer.ci.build.deploy.count),'deploys')"
                        }
                    ],
                    "aliasColors": {
                        "failure": "rgb(226, 77, 66)",
                        "success": "rgb(126, 178, 109)",
                        "deploys": "rgb(234, 184, 57)"
                    },
                    "aliasYAxis": {
                        "deploys": 2
                    },
                    "title": "CI status"
                }
            ]
        }
    ],
    "editable": true,
    "failover": false,
    "panel_hints": true,
    "style": "dark",
    "pulldowns": [
        {
            "type": "filtering",
            "collapse": false,
            "notice": false,
            "enable": false
        }
    ],
    "nav": [
        {
            "type": "timepicker",
            "collapse": false,
            "notice": false,
            "enable": true,
            "status": "Stable",
            "time_options": [
                "5m",
                "15m",
                "1h",
                "6h",
                "12h",
                "24h",
                "2d",
                "7d",
                "30d"
            ],
            "refresh_intervals": [
                "5s",
                "10s",
                "30s",
                "1m",
                "5m",
                "15m",
                "30m",
                "1h",
                "2h",
                "1d"
            ],
            "now": true
        }
    ],
    "loader": {
        "save_gist": false,
        "save_elasticsearch": true,
        "save_local": true,
        "save_default": true,
        "save_temp": true,
        "save_temp_ttl_enable": true,
        "save_temp_ttl": "30d",
        "load_gist": false,
        "load_elasticsearch": true,
        "load_elasticsearch_size": 20,
        "load_local": false,
        "hide": false
    },
    "refresh": "30s"
}
